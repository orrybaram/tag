<%= form_tag 'session_path', :class => "form-inline" do %>
    <fieldset>
      <%= text_field_tag :username, params[:username], :placeholder => "Username", :class => "login input-text"  %>
      <%= password_field_tag :password, params[:password], :placeholder => "Password", :class => "login input-text" %>
      <%= submit_tag "Login", :class => 'btn' %>
  </fieldset>
<% end %>


